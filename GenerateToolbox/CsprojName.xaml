<Window x:Class="GenerateToolbox.CsprojName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenerateToolbox"
        mc:Ignorable="d"
        WindowStyle="None"
        AllowsTransparency="True"
        BorderThickness="0"
        Background="{x:Null}"
        WindowStartupLocation="CenterScreen"
        Title="CsprojName" Height="185" Width="300">
    <Grid Background="#1E1E1E">
        <StackPanel>
            <TextBlock Text="项目名称" FontSize="11" Margin="5" Foreground="White"/>
            <TextBox x:Name="name_zh" Width="150" Height="30" HorizontalAlignment="Left" Margin="5,0,0,0" Background="#333337" Foreground="White" BorderThickness="0"/>
            <TextBlock Text="项目英文" FontSize="11" Margin="5" Foreground="White"/>
            <TextBox x:Name="name_eng" Width="150" Height="30" HorizontalAlignment="Left" Margin="5,0,0,0" Background="#333337" Foreground="White" BorderThickness="0"/>
        </StackPanel>
        <Button x:Name="btn" Width="100" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,10,10" Content="创建(C)" Click="Btn_Click" FontSize="13" FontFamily=".PingFang SC,Microsoft YaHei UI" Style="{DynamicResource SquareButtonStyle}"/>

    </Grid>
</Window>
